{
  "name": "brainsoft-pokemon-api",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "mikro-orm": "dotenv -c development mikro-orm",
    "mikro-orm:seed": "yarn mikro-orm migration:fresh && yarn mikro-orm seeder:run",
    "test:docker": "docker compose run api yarn test",
    "test": "jest --coverage",
    "lint": "ts-standard --fix 'src/**/*.ts' 'test/**/*.ts'",
    "start": "fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "yarn build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"yarn:watch:ts\" \"yarn dev:start\"",
    "dev:start": "fastify start --ignore-watch='.ts$' -w -l info -P dist/app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/sensible": "^5.0.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@mikro-orm/cli": "^6.0.5",
    "@mikro-orm/core": "^6.0.5",
    "@mikro-orm/migrations": "^6.0.5",
    "@mikro-orm/postgresql": "^6.0.5",
    "@mikro-orm/reflection": "^6.0.5",
    "@mikro-orm/seeder": "^6.0.5",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "fastify": "^4.0.0",
    "fastify-cli": "^6.0.1",
    "fastify-plugin": "^4.0.0",
    "fastify-type-provider-zod": "^1.1.9",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.2.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.4.4",
    "@types/tap": "^15.0.5",
    "c8": "^9.1.0",
    "concurrently": "^8.2.2",
    "dotenv-cli": "^7.3.0",
    "ts-node": "^10.4.0",
    "ts-standard": "^12.0.2"
  },
  "packageManager": "yarn@4.0.2",
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  },
  "ts-standard": {
    "project": "./tsconfig.json",
    "ignore": [
      "src/migrations/**"
    ]
  }
}
